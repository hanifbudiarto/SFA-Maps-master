<Window x:Class="SalesForceMap.MainWindow"
        xmlns:prg="clr-namespace:SalesForceMap"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SalesForceMap"
        mc:Ignorable="d"
        Title="SFA Tracking" Height="580" Width="880" MinHeight="450" MinWidth="550"   KeyUp="Window_KeyUp" PreviewKeyDown="Window_PreviewKeyDown" WindowStartupLocation="CenterScreen" Closing="Window_Closing" >




    <Window.Resources>

        <Style x:Key="ZoomUpButton" TargetType="Button">
            <Setter Property="Width" Value="23.5" />
            <Setter Property="Height" Value="25.5" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid Width="23.5" Height="25.5">
                            <Image Source="pack://application:,,,/Gambar/zoom_1.png" Stretch="Fill" />
                            <ContentPresenter HorizontalAlignment="Center" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <DropShadowEffect ShadowDepth="2" />
                                    </Setter.Value>
                                </Setter>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ZoomDownButton" TargetType="Button">
            <Setter Property="Width" Value="23.5" />
            <Setter Property="Height" Value="25.5" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid Width="23.5" Height="25.5">
                            <Image Source="pack://application:,,,/Gambar/zoom_.png" Stretch="Fill" />
                            <ContentPresenter HorizontalAlignment="Center" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Effect">
                                    <Setter.Value>
                                        <DropShadowEffect ShadowDepth="2" />
                                    </Setter.Value>
                                </Setter>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>



        <Style x:Key="TabItemStyle1" TargetType="{x:Type TabItem}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TabItem}">
                        <Border x:Name="Border"  BorderThickness="0,0,0,2" CornerRadius="5,5,0,0"
                        Padding="15,5,20,5" Margin="0,0,0,0" BorderBrush="Gainsboro" >
                            <ContentPresenter x:Name="ContentSite" ContentSource="Header"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter Property="TextElement.Foreground" TargetName="ContentSite" Value="White" />
                                <Setter Property="TextElement.FontSize" TargetName="ContentSite" Value="16"/>
                                <Setter TargetName="Border" Property="Background" Value="{x:Null}"/>
                                <Setter TargetName="Border" Property="BorderBrush" Value="White"/>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="False">
                                <Setter Property="TextElement.Foreground" TargetName="ContentSite" Value="#dddddd"/>
                                <Setter Property="TextElement.FontSize" TargetName="ContentSite" Value="16"/>
                                <Setter TargetName="Border" Property="Background" Value="{x:Null}" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="{x:Null}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="ButtonStyle1" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#FF1479BF"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderThickness="0,0,0,3" BorderBrush="#FF0080D8"  CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#FF0080D8"/>
                </Trigger>
            </Style.Triggers>
        </Style>


        <Style x:Key="ButtonStyle2" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#FFF9F9F9"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderThickness="3,3,3,3" BorderBrush="#FF7EBAE4"  CornerRadius="2">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#FFEAEFF3"/>
                </Trigger>
            </Style.Triggers>
        </Style>


        <Style x:Key="ButtonStyle3" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#FF3092E4"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderThickness="0,0,0,3" BorderBrush="#FF0080D8"  CornerRadius="2">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#FF0080D8"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="ButtonStyle4" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#887D8891"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderThickness="0,0,0,3" BorderBrush="#8871787C"  CornerRadius="2">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#886E6E6E"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="ButtonStyle5" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#B255EA5C"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderThickness="0,0,0,3" BorderBrush="#8871787C"  CornerRadius="2">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#B260BF11"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="TextBoxStyle1" BasedOn="{x:Null}" TargetType="{x:Type TextBox}">
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="AllowDrop" Value="true"/>
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="Template">
                <Setter.Value>

                    <ControlTemplate TargetType="{x:Type TextBox}">
                        <Grid>
                            <Border x:Name="bg"   BorderBrush="#ffffff" BorderThickness="0,0,0,1"/>
                            <ScrollViewer x:Name="PART_ContentHost" Margin="5,0,0,0" VerticalAlignment="Center" />
                            <Label Margin="5,0,0,0" x:Name="WaterMarkLabel" Content="{TemplateBinding Tag}" VerticalAlignment="Center"
                               Visibility="Collapsed" Foreground="#cccccc" FontFamily="Arial"/>
                        </Grid>

                        <!--<Border x:Name="bg" BorderBrush="#ffffff" BorderThickness="0,0,0,1">
                            <ScrollViewer x:Name="PART_ContentHost" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                        </Border>-->

                        <ControlTemplate.Triggers>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="Text" Value=""/>
                                </MultiTrigger.Conditions>
                                <Setter Property="Visibility" TargetName="WaterMarkLabel" Value="Visible"/>
                            </MultiTrigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Foreground" Value="DimGray"/>
                            </Trigger>

                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="BorderBrush" TargetName="bg" Value="#ffffff"/>
                                <Setter Property="BorderThickness" TargetName="bg" Value="0,0,0,2"/>
                            </Trigger>
                            <Trigger Property="IsFocused" Value="True">
                                <Setter Property="BorderBrush" TargetName="bg" Value="#FF77CEFF"/>
                                <Setter Property="BorderThickness" TargetName="bg" Value="0,0,0,2"/>
                            </Trigger>

                        </ControlTemplate.Triggers>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>



        <Style x:Key="PasswordBoxStyle1" BasedOn="{x:Null}" TargetType="{x:Type PasswordBox}">
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="AllowDrop" Value="true"/>
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="Template">
                <Setter.Value>

                    <ControlTemplate TargetType="{x:Type PasswordBox}">
                        <Grid>
                            <Border x:Name="bg"   BorderBrush="#ffffff" BorderThickness="0,0,0,1"/>
                            <ScrollViewer x:Name="PART_ContentHost" Margin="5,0,0,0" VerticalAlignment="Center" />
                            <Label Margin="5,0,0,0" x:Name="WaterMarkLabel" Content="{TemplateBinding Tag}" VerticalAlignment="Center"
                               Visibility="Collapsed" Foreground="#cccccc" FontFamily="Arial"/>
                        </Grid>

                        <!--<Border x:Name="bg" BorderBrush="#ffffff" BorderThickness="0,0,0,1">
                            <ScrollViewer x:Name="PART_ContentHost" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                        </Border>-->

                        <ControlTemplate.Triggers>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <!--<Condition Property="Password" Value=""/>-->
                                </MultiTrigger.Conditions>
                                <Setter Property="Visibility" TargetName="WaterMarkLabel" Value="Visible"/>
                            </MultiTrigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Foreground" Value="DimGray"/>
                            </Trigger>

                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="BorderBrush" TargetName="bg" Value="#ffffff"/>
                                <Setter Property="BorderThickness" TargetName="bg" Value="0,0,0,2"/>
                            </Trigger>
                            <Trigger Property="IsFocused" Value="True">
                                <Setter Property="BorderBrush" TargetName="bg" Value="#FF77CEFF"/>
                                <Setter Property="BorderThickness" TargetName="bg" Value="0,0,0,2"/>
                            </Trigger>

                        </ControlTemplate.Triggers>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Storyboard x:Key="sbShowLeftMenu" >
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="-300,10,0,115" To="0,10,0,115" DecelerationRatio=".9" Duration="0:0:0.5" />
        </Storyboard>
        <Storyboard x:Key="sbHideLeftMenu">
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,10,0,115" To="-300,10,0,115" AccelerationRatio=".9" Duration="0:0:0.5" />
        </Storyboard>


        <Storyboard x:Key="sbShowTopMenu" >
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,-100,0,0" To="0,0,0,0" DecelerationRatio=".9" Duration="0:0:1" />
        </Storyboard>
        <Storyboard x:Key="sbHideTopMenu">
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,0,0,0" To="0,-100,0,0" AccelerationRatio=".9" Duration="0:0:1" />
        </Storyboard>


        <Storyboard x:Key="sbShowRightMenu" >
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,10,-300,115" To="0,10,0,115" DecelerationRatio=".9" Duration="0:0:0.5" />
        </Storyboard>
        <Storyboard x:Key="sbHideRightMenu">
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,10,0,115" To="0,10,-300,115" AccelerationRatio=".9" Duration="0:0:0.5" />
        </Storyboard>


        <Storyboard x:Key="sbShowBottomMenu" >
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,0,0,-100" To="0,0,0,0" DecelerationRatio=".9" Duration="0:0:1" />
        </Storyboard>
        <Storyboard x:Key="sbHideBottomMenu">
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,0,0,0" To="0,0,0,-100" AccelerationRatio=".9" Duration="0:0:1" />
        </Storyboard>

    </Window.Resources>


    <Grid>

        <TabControl x:Name="tabcontrol_utama">
            <TabItem Width="0" Height="0" >
                <Grid Margin="-10" Opacity="1">
                    <!--<Grid.Background>
                        <ImageBrush ImageSource="pack://application:,,,/Gambar/bgxblur.jpg" Stretch="UniformToFill" TileMode="Tile"/>
                    </Grid.Background>-->

                    <Grid>
                        <Grid.Background>
                            <ImageBrush ImageSource="pack://application:,,,/Gambar/bg.jpg" Stretch="UniformToFill" TileMode="Tile"/>
                        </Grid.Background>
                    </Grid>

                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Height="300" 
                      >



                        <Button x:Name="button_signin" Style="{DynamicResource ButtonStyle1}" Content="Sign In" Canvas.Left="10" Canvas.Top="165" Width="299" Click="button_signin_Click" TabIndex="3" FontSize="16" FontWeight="Normal" Margin="0,203,0,0" Height="40" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="White"/>
                        <PasswordBox x:Name="textbox_password" Tag="Password" Style="{DynamicResource PasswordBoxStyle1}" VerticalContentAlignment="Center" Canvas.Left="10"   Canvas.Top="102" Width="299"  TabIndex="2" FontSize="13" KeyUp="textbox_password_KeyUp" Margin="0,154,0,0" Height="33" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="White" Foreground="White" CaretBrush="White" BorderThickness="0,0,0,1" PasswordChanged="textbox_password_PasswordChanged">
                            <PasswordBox.InputBindings>
                                <KeyBinding Key="Return" Command="" />
                            </PasswordBox.InputBindings>
                        </PasswordBox>
                        <TextBox x:Name="textbox_email"  Tag="Username" Style="{DynamicResource TextBoxStyle1}" VerticalContentAlignment="Center" Height="33" Canvas.Left="10" TextWrapping="Wrap" Text="" Canvas.Top="64" Width="299"  TabIndex="1" FontSize="13" AcceptsReturn="False" KeyUp="textbox_email_KeyUp" Margin="0,116,0,0" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="White" Foreground="White" CaretBrush="White" BorderThickness="0,0,0,1" MaxLines="1">
                            <TextBox.InputBindings>
                                <KeyBinding Key="Return" Command="" />
                            </TextBox.InputBindings>

                        </TextBox>
                        <!--<Label Content="Forgot password?"  Width="131" HorizontalAlignment="Left" Height="29" VerticalAlignment="Top" Margin="1,250,0,0" Foreground="White"  />-->
                        <Image Canvas.Left="10" Canvas.Top="10" Source="pack://application:,,,/Gambar/logo.png" Margin="0,26,-1,0" Height="60" VerticalAlignment="Top" />

                    </Grid>
                    <Button x:Name="btn_preference" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="20"  Style="{StaticResource ButtonStyle4}" Foreground="#ffffff" FontWeight="Bold" Click="btn_preference_Click" >
                        <Button.Content>
                            <Grid>
                                <StackPanel Orientation="Horizontal">
                                    <Image  Margin="0,0,0,0" 
									    Source="pack://application:,,,/Gambar/ic_settings_applications_white_48dp.png" Height="25" Width="25" HorizontalAlignment="Left"
                                         Stretch="Uniform">
                                    </Image>
                                    <Label Content="Preference" Foreground="#ffffff"></Label>
                                </StackPanel>
                               
                            </Grid>
                        </Button.Content>
                    </Button>

                </Grid>
            </TabItem>
            <TabItem Width="0" Height="0" >
                <Grid>
                    <Grid x:Name="canvas_atas" Margin="-10" Height="60"  VerticalAlignment="Top" Background="#FF3DACE0">

                        <StackPanel Margin="5,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal">


                            <StackPanel Margin="8"   Orientation="Vertical">
                                <StackPanel Orientation="Horizontal">
                                    <!--<Rectangle Margin="5,0,0,0" Width="36" Height="36">
                                        <Rectangle.Resources>
                                            <SolidColorBrush x:Key="BlackBrush" Color="White" />
                                        </Rectangle.Resources>
                                        <Rectangle.Fill>
                                            <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_calendar}" />
                                        </Rectangle.Fill>
                                    </Rectangle>-->
                                    <Image  Margin="5,0,0,0" 
									    Source="pack://application:,,,/Gambar/ic_event_note_white_48dp.png" Height="40"
                                         Stretch="Uniform">
                                    </Image>

                                    <StackPanel Margin="10,0,0,0" Orientation="Vertical">
                                        <Label x:Name="label_tanggal" Content="No date selected" HorizontalAlignment="Left" Foreground="#ffffff"></Label>
                                        <Button x:Name="btn_selectdate" Margin="0,0,0,0" Width="70" HorizontalAlignment="Left" Content="Select" Style="{StaticResource ButtonStyle3}" Foreground="#ffffff" Click="btn_selectdate_Click"></Button>
                                    </StackPanel>

                                </StackPanel>
                            </StackPanel>




                            <StackPanel Margin="8"  Orientation="Vertical">
                                <StackPanel Orientation="Horizontal">
                                    <!--<Rectangle Margin="5,0,0,0" Width="36" Height="36">
                                        <Rectangle.Resources>
                                            <SolidColorBrush x:Key="BlackBrush" Color="White" />
                                        </Rectangle.Resources>
                                        <Rectangle.Fill>
                                            <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_user}" />
                                        </Rectangle.Fill>
                                    </Rectangle>-->
                                    <Image  Margin="5,0,0,0" 
									    Source="pack://application:,,,/Gambar/ic_person_outline_white_48dp.png" Height="40"
                            Stretch="Uniform">
                                    </Image>



                                    <StackPanel Margin="10,0,0,0" Orientation="Vertical">
                                        <Label x:Name="label_sales" Content="No sales selected" HorizontalAlignment="Left" Foreground="#ffffff"></Label>
                                        <Button x:Name="btn_selectsales" Margin="0,0,0,0" Width="70" HorizontalAlignment="Left" Content="Select" Style="{StaticResource ButtonStyle3}" Foreground="#ffffff" Click="btn_selectsales_Click"></Button>
                                    </StackPanel>

                                </StackPanel>
                            </StackPanel>


                        </StackPanel>

                        <Button x:Name="btn_proses" Content="PROCESS" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,20,0" Width="100" Height="30" Style="{StaticResource ButtonStyle5}" Foreground="#ffffff" FontWeight="Bold" Click="btn_proses_Click"></Button>

                    </Grid>






                    <Grid Margin="-10,50,-10,0">

                        <Grid>
                            <local:Map x:Name="MainMap" Zoom="10" MaxZoom="24" MinZoom="1" />
                        </Grid>
                        
                        <StackPanel Panel.ZIndex="2" Name="pnlLeftMenu" Orientation="Horizontal"  HorizontalAlignment="Left" Margin="-300,10,0,115">
                            <Border BorderBrush="#FFCDCDCD" BorderThickness="2" Width="300" Background="#eeffffff" >
                                <Grid>
                                    <Grid Height="40" VerticalAlignment="Top" Background="#FF6098A0" Margin="0,-2,0,0">
                                        <Image  Margin="10,0,0,0" 
									    Source="pack://application:,,,/Gambar/ic_format_list_bulleted_white_48dp.png" Height="25" Width="25" HorizontalAlignment="Left"
                                         Stretch="Uniform">
                                        </Image>
                                        <Label Content="Travel details" Foreground="#ffffff" FontSize="16"  VerticalAlignment="Center" Margin="40,0,0,0"/>
                                    </Grid>

                                    <ListView x:Name="list_daftar" BorderThickness="0" Background="#FFffffff"  ScrollViewer.HorizontalScrollBarVisibility="Disabled"  Margin="0,40,0,0"  SelectionChanged="list_daftar_SelectionChanged" >
                                        <ListView.ItemContainerStyle>
                                            <Style  TargetType="{x:Type ListViewItem}">
                                                <Setter Property="Template">
                                                    <Setter.Value>
                                                        <ControlTemplate TargetType="{x:Type ListViewItem}">
                                                            <Border x:Name="border" BorderThickness="0,0,0,0" Background="Transparent">
                                                                <VisualStateManager.VisualStateGroups>
                                                                    <VisualStateGroup x:Name="CommonStates">
                                                                        <VisualState x:Name="Normal" />
                                                                        <VisualState x:Name="Disabled" />
                                                                    </VisualStateGroup>
                                                                    <VisualStateGroup x:Name="SelectionStates">
                                                                        <VisualState x:Name="Unselected" />
                                                                        <VisualState x:Name="Selected">
                                                                            <Storyboard>
                                                                                <ColorAnimationUsingKeyFrames Storyboard.TargetName="border"
														Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)">
                                                                                    <EasingColorKeyFrame KeyTime="0" Value="LightBlue" />
                                                                                </ColorAnimationUsingKeyFrames>
                                                                            </Storyboard>
                                                                        </VisualState>
                                                                        <VisualState x:Name="SelectedUnfocused">
                                                                            <Storyboard>
                                                                                <ColorAnimationUsingKeyFrames Storyboard.TargetName="border"
														Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)">
                                                                                    <EasingColorKeyFrame KeyTime="0" Value="SkyBlue" />
                                                                                </ColorAnimationUsingKeyFrames>
                                                                            </Storyboard>
                                                                        </VisualState>
                                                                    </VisualStateGroup>
                                                                </VisualStateManager.VisualStateGroups>
                                                                <ContentPresenter/>
                                                            </Border>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </ListView.ItemContainerStyle>

                                        <ListView.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <!--<StackPanel/>
                   High memory usage-->
                                                <VirtualizingStackPanel Margin="0,0,0,80">
                                                    <!--Less memeory usage-->
                                                </VirtualizingStackPanel>
                                            </ItemsPanelTemplate>
                                        </ListView.ItemsPanel>

                                        <ListView.ItemTemplate>
                                            <DataTemplate>
                                                <Grid  VerticalAlignment="Top" Margin="5">
                                                    <StackPanel Orientation="Horizontal">
                                                        <Grid Width="35" Height="35" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                                            <Border Canvas.Left="55"
					                                                Canvas.Top="30"
					                                                Width="35"
					                                                Height="35"
					                                                Margin="0"
					                                                VerticalAlignment="Top"
					                                                BorderBrush="#33475285"
					                                                BorderThickness="2"
					                                                CornerRadius="300">
                                                                <Border.Effect>
                                                                    <DropShadowEffect BlurRadius="1"
							                                            Opacity=".5"
							                                            ShadowDepth="2" />
                                                                </Border.Effect>
                                                                <Border
						                                            BorderBrush="#FFffffff"
						                                            BorderThickness="0"
						                                            CornerRadius="300">
                                                                    <Grid>
                                                                        <Border x:Name="Masks"
								                                                Background="White"
								                                                BorderBrush="#FF3B5998"
								                                                BorderThickness="0"
								                                                CornerRadius="300"/>
                                                                        <StackPanel>
                                                                            <StackPanel.OpacityMask>
                                                                                <VisualBrush Visual="{Binding ElementName=Masks}" />
                                                                            </StackPanel.OpacityMask>
                                                                            <Image RenderOptions.BitmapScalingMode="HighQuality"
									Source="pack://application:,,,/Gambar/avatar_1_raster.png" />
                                                                        </StackPanel>
                                                                    </Grid>
                                                                </Border>
                                                            </Border>



                                                        </Grid>

                                                    </StackPanel>
                                                    <Grid Margin="45,0,0,0" VerticalAlignment="Top" >
                                                        <StackPanel Orientation="Vertical">
                                                            <StackPanel Orientation="Horizontal">
                                                                <Label FontSize="10"  Foreground="#FF777777"   VerticalAlignment="Center" Padding="5,0,5,0">
                                                                    <TextBlock TextTrimming="CharacterEllipsis" Text="Checkin time" Foreground="#FF777777"/>
                                                                </Label>
                                                                <Label FontSize="10"  Foreground="#FF777777"   VerticalAlignment="Center" Padding="5,0,5,0">
                                                                    <TextBlock TextTrimming="CharacterEllipsis" Text="{Binding StrCheckin}" Foreground="#FF777777"/>
                                                                </Label>
                                                            </StackPanel>


                                                            <Label FontSize="13"  Foreground="#FF444444"  Padding="5,3,5,3" VerticalAlignment="Center" >
                                                                <TextBlock TextTrimming="CharacterEllipsis" Text="{Binding NamaPelanggan}" Foreground="#FF444444"/>
                                                            </Label>

                                                            <StackPanel Orientation="Horizontal">
                                                                <Label FontSize="10"  Foreground="#FFffffff" HorizontalAlignment="Left" Padding="5,3,5,3" Margin="0,0,0,0" Background="{Binding ColorStatus}">
                                                                    <TextBlock TextTrimming="CharacterEllipsis" Text="{Binding Keterangan}" Foreground="#FFffffff"/>
                                                                </Label>
                                                                <Label FontSize="10" Visibility="{Binding IsVisibleKunjungan}"  Foreground="#FFffffff" HorizontalAlignment="Left" Padding="5,3,5,3" Margin="5,0,0,0" Background="#FF4593A4">
                                                                    <TextBlock TextTrimming="CharacterEllipsis" Text="{Binding Kunjunganke}" Foreground="#FFffffff"/>
                                                                </Label>
                                                            </StackPanel>

                                                            <DockPanel Background="LightGray" Width="Auto" Height="1" Margin="0,5,0,0"></DockPanel>
                                                        </StackPanel>
                                                    </Grid>
                                                </Grid>
                                                
                                            </DataTemplate>
                                        </ListView.ItemTemplate>
                                    </ListView>
                                </Grid>
